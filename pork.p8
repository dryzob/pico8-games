--[[
	rougelike game
	tutorial found on youtube
	by lazy devs
]]
function _init()
	t=0
	p_ani={240,241,242,243}
	p_dir={0,1,2,3}
	
	_upd=upd_game
	_drw=drw_game
	startgame()
end

function _update60()
	t+=1
	_upd()
end

function _draw()
	_drw()
end

function startgame()
	p_x=3
	p_y=5
	p_ox=0
	p_oy=0
end
-->8
-- updates

function upd_game()
	if btnp(⬅️) then
	 p_x-=1
	 p_ox=8
	 _upd=upd_pturn
	end
	if btnp(➡️) then
	 p_x+=1
	 p_ox=-8
	 _upd=upd_pturn
	end
	if btnp(⬆️) then
	 p_y-=1
	 p_oy=8
	 _upd=upd_pturn
	end
	if btnp(⬇️) then
	 p_y+=1
	 p_oy=-8
	 _upd=upd_pturn
	end
end

function upd_pturn()
	if p_ox>0 then
		p_ox-=2
	end
	if p_oy>0 then
		p_oy-=2
	end
	if p_ox<0 then
		p_ox+=2
	end
	if p_oy<0 then
		p_oy+=2
	end
	
	if p_ox==0 and p_oy==0 then
		_upd=upd_game
	end
end

function upd_gameover()

end
-->8
-- draws

function drw_game()
	cls(0)
	map()
	drawspr(getframe(p_ani),p_x*8+p_ox,p_y*8+p_oy,10)
end

function drw_gameover()

end
-->8
-- tools

function getframe(ani)
	return ani[flr(t/8)%#ani+1]
end

function drawspr(_spr,_x,_y,_c)
	palt(0,false)
	pal(6,_c)
	spr(_spr,_x,_y)
	pal()
end
