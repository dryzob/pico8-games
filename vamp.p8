-- gemeral functions

wall=0x99
inct=0x20

function hit(x,y,w,h,col)
	local collide=false
	
	for i=x,x+w do
		if((fget(mget(i/8,y/8))&col)==col)or
			((fget(mget(i/8,(y+h)/8))&col)==col)then
		 	collide=true
		end
	end
	
	for i=y,y+h do
  if((fget(mget(x/8,i/8))&col)==col)or
   ((fget(mget((x+w)/8,i/8))&col)==col)then
    collide=true
  end
 end
 return collide
end

function move(act)
	local dx=0
	local dy=0

	if(btn(â¬…ï¸)) dx -= act.spe
	if(btn(âž¡ï¸)) dx += act.spe
	if(btn(â¬†ï¸)) dy -= act.spe
	if(btn(â¬‡ï¸)) dy += act.spe
	
	-- colision
	if hit(act.x+dx,act.y,
					act.w,act.h,wall) then
		dx=0
	end
	if hit(act.x,act.y+dy,
					act.w,act.h,wall) then
		dy=0
	end
	
	act.x+=dx
	act.y+=dy
end
